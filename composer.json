{
  "require": {
    "craftcms/ckeditor": "1.0.0-beta.3",
    "craftcms/cms": "^3.4.0",
    "craftcms/simple-text": "2.0.2",
    "dolphiq/iconpicker": "1.0.5",
    "hashtagerrors/craft-pagination": "1.0.2",
    "solspace/craft-freeform": "3.6.7",
    "studioespresso/craft-dumper": "2.0.0",
    "verbb/image-resizer": "2.0.8",
    "vlucas/phpdotenv": "^3.4.0"
  },
  "require-dev": {
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
