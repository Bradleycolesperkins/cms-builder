                                    <div class="gmap_canvas">
                                        <iframe
                                                frameborder="0"
                                                scrolling="no"
                                                marginheight="0"
                                                marginwidth="0"
                                                src="https://maps.google.com/maps?q={{ data['latitude'] }},{{ data['longitude'] }}&hl=en&z=18&amp;output=embed"
                                        >
                                        </iframe>
                                    </div>
                                </div>
                                {% set mapCss %}
                                    .gmap_canvas {
                                        width: 100% !important;
                                        height: 500px !important;
                                    }
                                    .gmap_canvas iframe {
                                        width: 100% !important;
                                        height: 500px !important;
                                    }
                                {% endset %}

