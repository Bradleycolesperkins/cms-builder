{% extends 'template_2/_layout' %}
{% set urlItems = craft.request.getUrl() | split('/') %}
{% set propertyId = urlItems | last %}


{% set url = '/api/single-property?id='~propertyId~'' %}
{% set client = {
    base_uri : 'http://pm-premier-api.azurewebsites.net',
    timeout : 10
} %}

{% set options = {} %}

{% set request = fetch(client, 'GET', url, options, false) %}

{% if(request.statusCode == 200) %}
    {% set data = request.body | json_decode() %}
{% endif %}

{% block main %}
    {% if (data is defined and data is not null) %}

        <main class="page-content pt--60 pt_md--40 pt_sm--20">
        <!-- Start Single Product -->
        <div class="brook-single-product plr--160 plr_lg--100 plr_md--50 plr_sm--50">
            <div class="row">
                <div class="col-lg-6 col-md-6 col-sm-12 col-12">
                    <div class="single-product-left-side">
                        <div class="product__details__container">
                            <div class="tab_container big_img_container">
                                {% if data['property_images'] %}
                                    {% for propertyImage in data['property_images'] %}
                                        <div class="big_img tab-pane fade
                                        {% if loop.first %} show active {% endif %}
                                         " id="img{{ propertyImage['id'] }}" role="tabpanel">
                                            <img class="w-100" src="{{ propertyImage['imageURL'] }}" alt="{{ propertyImage['id'] }}">
                                        </div>
                                    {% endfor %}
                                {% endif %}
                            </div>
                            <div class="sm_roduct_nav nav nav-tabs" role="tablist">
                                {% if data['property_images'] %}
                                    {% for propertyImage in data['property_images'] %}
                                        <a {% if loop.first %} class="active" {% endif %}  id="img{{ propertyImage['id'] }}-tab" data-toggle="tab" href="#img{{ propertyImage['id'] }}" role="tab" aria-controls="img{{ propertyImage['id'] }}"
                                                {% if loop.first %} aria-selected="true" {% else %} aria-selected="false" {% endif %}>
                                            <img src="{{ propertyImage['imageURL'] }}" alt="{{ propertyImage['id'] }}-click">
                                        </a>
                                    {% endfor %}
                                {% endif %}
                            </div>

                            <div class="brook-call-to-action primary-background mt--40 ptb--20 border-radius-10">
                                <div class="container">
                                    <div class="row align-items-center">
                                        <div class="col-lg-8 col-sm-6 col-12">
                                            <div class="call-content text-center text-sm-left">
                                                <h3 class="heading heading-h3 text-white wow move-up  animate" style="visibility: visible; opacity: 0;">Schedule a Viewing</h3>
                                            </div>
                                        </div>
                                        <div class="col-lg-4 col-sm-6 col-12">
                                            <div class="call-btn text-center text-sm-right mt_mobile--20 wow move-up  animate" style="visibility: visible; opacity: 0;">
                                                <button class="brook-btn bk-btn-white text-theme btn-sd-size btn-rounded" id="bookButton">Book Here</button>
                                                {% set myJs %}
                                                    $("#bookButton").on("click", function(){
                                                    $('.description_nav.nav-tabs > a.active').removeClass('active');
                                                    $('.description_nav.nav-tabs > a.show').removeClass('show');

                                                    $('.description_nav.nav-tabs > a#nav-contact').addClass('active');
                                                    $('.description_nav.nav-tabs > a#nav-contact').addClass('show');

                                                    $('.tab_container.review_container > .tab-pane.active').removeClass('active');
                                                    $('.tab_container.review_container > .tab-pane.show').removeClass('show');

                                                    $('.tab_container.review_container > .tab-pane#contact').addClass('active');
                                                    $('.tab_container.review_container > .tab-pane#contact').addClass('show');


                                                    $([document.documentElement, document.body]).animate({
                                                    scrollTop: $("#elementtoScrollToID").offset().top - 180
                                                    }, 2000);

                                                    });
                                                {% endset %}
                                                {% includeJsFile "https://ajax.googleapis.com/ajax/libs/jquery/2.1.1/jquery.min.js" %}
                                                {% includeJs myJs %}
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>


                            {% if data['floor_plan_images'] %}

                                <!-- Start Gallery Area -->
                                <div class="brook-gallery-area ptb--40 ptb-md--40 ptb-sm--20 bg_color--1 basic-thine-line">
                                    <div class="container">
                                        <div class="row">
                                            <div class="col-lg-12">
                                                <div class="mb--40">
                                                    <h2 class="heading heading-h6">Floorplans</h2>
                                                </div>

                                                <div class="gallery-wrapper gallery-grid grid-cols-{{ data['floor_plan_images']|length }}" id="animated-thumbnials2">

                                                    {% for floorPlanImage in data['floor_plan_images'] %}
                                                        <!-- Start Single Gallery -->
                                                        <a class="gallery" href="{{ floorPlanImage['url'] }}">
                                                            <div class="thumb">
                                                                <img src="{{ floorPlanImage['url'] }}" alt="{{ floorPlanImage['id'] }}-floorplan">
                                                            </div>
                                                            <div class="hover-overlay">
                                                                <div class="inner">
                                                                    <span class="ion ion-android-add"></span>
                                                                </div>
                                                            </div>
                                                        </a>
                                                        <!-- End Single Gallery -->
                                                    {% endfor %}

                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <!-- End Gallery Area -->
                            {% endif %}

                        </div>
                    </div>
                </div>
                <div class="col-lg-6 col-md-6 col-sm-12 col-12 mt_sm--40">
                    <div class="single-product-details-side">
                        <div class="product-details">
                            <div class="inner">
                                <div class="header">
                                    {#<div class="product-badges">#}
                                    {#    <span>H.header__digital--broadcastot</span>#}
                                    {#</div>#}
                                    <h4 class="heading heading-h4">
                                        {% if data['addressName'] %}{{ data['addressName']  }}{% endif  %}
                                        {% if data['addressNumber'] %}{{ data['addressNumber']  }}{% endif  %}
                                        {% if data['addressStreet'] %}{{ data['addressStreet']  }}{% endif  %}
                                        {% if data['address2'] %}{{ data['address2']  }}{% endif  %}
                                        {% if data['address3'] %}{{ data['address3']  }}{% endif  %}
                                        {% if data['address4'] %}{{ data['address4']  }}{% endif  %}
                                        {% if data['addressPostcode'] %}{{ data['addressPostcode']  }}{% endif  %}
                                    </h4>
                                </div>
                                <div class="price" style="display:block;clear: both;overflow: hidden;position: relative;width: 100%;">
                                    <p class="theme-color float-left">
                                        {% switch data['availability'] %}
                                        {% case '1' %}
                                            On Hold
                                        {% case '2' %}
                                            {% if data['availability'] is not null and data['department'] == "Lettings" %}
                                                To Let
                                            {% else %}
                                                For Sale
                                            {% endif %}
                                        {% case '3' %}
                                            {% if data['availability'] is not null and data['department'] == "Lettings" %}
                                                {#References Pending#}
                                                Let Agreed
                                            {% else %}
                                                Under Offer
                                            {% endif %}
                                        {% case '4' %}
                                            {% if data['availability'] is not null and data['department'] == "Lettings" %}
                                                Let Agreed
                                            {% else %}
                                                Sold STC
                                            {% endif %}
                                        {% case '5' %}
                                            {% if data['availability'] is not null and data['department'] == "Lettings" %}
                                                Let {#  #}
                                            {% else %}
                                                Sold
                                            {% endif %}
                                        {% case '6' %}
                                            {% if data['availability'] is not null and data['department'] == "Lettings" %}
                                                Withdrawn
                                            {% else %}
                                            {% endif %}
                                        {% case '7' %}
                                            Withdrawn
                                        {% default %}
                                            Other
                                        {% endswitch %}
                                    </p>
                                    <p class="theme-color float-right">
                                        {% if data['department'] == "Lettings" %}
                                            £{{ data['rent']|number_format(0, '', ',') }}
                                            <span class="new-price"> Per Month</span>
                                        {% else %}
                                            £{{ data['price']|number_format(0, '', ',') }}
                                        {% endif %}
                                    </p>
                                </div>
                                <div class="description">
                                    <p class="bk_pra">
                                        {% if data['mainSummary'] %}{{ data['mainSummary'] }}{% endif  %}
                                    </p>
                                </div>
                                <div class="product-meta quenty-container">
