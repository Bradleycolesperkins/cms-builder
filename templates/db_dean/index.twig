{% extends 'db_dean/_layout' %}

{% block main %}
   <div class="cs-main-wrapp__inner">
      <div class="container padd-only-xs">
         {% include '/template_1/_components/_full_screen_image_banner'  with {'title': companyInformation.companySlogan|upper,'subtitle': '','imageUrl': '/assets/uploads/1590426988_DSC00638.jpg'} %}

         <div class="row-full-width clearfix"></div>
         <div class="row row-fluid  padding-lg-105t padding-lg-105b padding-sm-70t padding-sm-70b">
            <div class="column_container col-sm-12 col-md-6 ">
