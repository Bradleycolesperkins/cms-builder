                                       <div class="single_image-wrapper   box_border_grey">
                                          <img width="400" height="235" src="/assets/uploads/1588841946_rsz_20160610_113046.jpg" class="single_image-img attachment-full" alt=" image" srcset="/assets/uploads/1588841946_rsz_20160610_113046.jpg 235w, /assets/uploads/1588841946_rsz_20160610_113046.jpg 150w, /assets/uploads/1588841946_rsz_20160610_113046.jpg 100w" sizes="(max-width: 235px) 100vw, 235px">
                                       </div>
                                    </figure>
                                 </div>
                              </div>
                           </div>
                        </div>
                     </div>
                  </div>
               </div>
            </div>
         </div>

         {% include '/template_1/_components/_services' with {'title': 'WHAT WE DO','subtitle': 'HERE YOU CAN SEE AND ESTIMATE','imageUrl': '/assets/uploads/1588690619_image7.jpg', 'hideLink': true} %}

         {% include '/template_1/_components/_portfolio' with {'title': 'PORTFOLIO','subtitle': 'CHECK OUT OUR','imageUrl': '/assets/uploads/1588690327_image5.jpg'} %}

         {% include '/template_1/_components/_clients' with {'title': 'CLIENTS','subtitle': 'WE HAVE A LONG HISTORY OF HAPPY CLIENTS','imageUrl': '/assets/uploads/1588690523_image3.jpg'} %}

         {% include '/template_1/_components/_testimonials' with {'title': 'TESTIMONIALS','subtitle': 'SOME KIND WORDS FROM OUR CLIENTS','imageUrl': '/assets/uploads/1588690327_image5.jpg'} %}

         {% include '/template_1/_components/_blog' with {'title': 'OUR BLOG','subtitle': 'ARTICLES OF INTEREST','imageUrl': '/assets/uploads/1588690324_image6.jpg'} %}



         
